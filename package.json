    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.4"
  },
  "peerDependencies": {
    "@adobe/htlengine": "^6.4.0",
    "webpack": "^5"
